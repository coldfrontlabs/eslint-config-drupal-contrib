{
  "name": "eslint-config-drupal-contrib",
  "version": "1.0.2",
  "description": "An ESLint configuration to match the Drupal standard for contrib modules and themes.",
  "main": "index.js",
  "scripts": {
    "commit": "npx git cz",
    "lint": "npx eslint --ext .js --ignore-path .gitignore .",
    "fix": "npm run lint -- --fix",
    "prerelease": "npm run lint",
    "release": "npx standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coldfrontlabs/eslint-config-drupal-contrib.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "drupal"
  ],
  "author": "Coldfront Labs Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coldfrontlabs/eslint-config-drupal-contrib/issues"
  },
  "homepage": "https://github.com/coldfrontlabs/eslint-config-drupal-contrib#readme",
  "dependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0"
  },
  "peerDependencies": {
    "eslint": "^6.0.0",
    "prettier": "^1.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "commitizen": "^4.0.3",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0"
  }
}
